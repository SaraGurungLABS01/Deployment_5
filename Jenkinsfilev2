pipeline {
  agent any
   stages {
    stage ('Clean') {
      steps {
        sh '''#!/bin/bash
        echo "command to run the pkill.sh"
        '''
     }
   }
 stage ('Deploy') {
  steps {
     sh '''#!/bin/bash
     echo "command to run the setup2.sh"
     scp setup2.sh ubuntu@34.238.52.125:/home/ubuntu
     ssh ubuntu@34.238.52.125 'bash -s </home/ubuntu/setup2.sh'
    '''
      }
    }
  }
}
